/**
*	if (sel=0) {a,b}={in,0}
*	else {a,b}={0,in}
/*

CHIP Dmux{
	IN in, sel;
	OUT a, b;

	PARTS:
	Not (in=sel, out=notsel);
	And (a=in, b=notsel, out=out1);
	And (a=in, b=sel, out=out2);
	
}
