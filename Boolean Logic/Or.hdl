/**
*	if (a=1, b=1) then out=1 
*	else out=0
/*

CHIP Or {
	IN a,b;
	OUT out;

	PARTS:
	Nand (a=a, b=a, out=out0);
	Nand (a=b, b=b, out=out1);
	Nand (a=out0, b-out1, out=out);
}
